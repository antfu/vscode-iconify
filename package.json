              "brandico",
              "entypo-social",
              "cryptocurrency",
              "map",
              "geo",
              "cif",
              "wi",
              "medical-icon"
            ]
          },
          "default": null,
          "description": "Collection IDs to be excluded for detection"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --external vscode",
    "dev": "pnpm build -- --watch",
    "prepare": "esno scripts/prepare.ts",
    "release": "esno scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.3.2",
    "@iconify/iconify": "^1.0.7",
    "@iconify/json": "^1.1.205",
    "@types/fs-extra": "^9.0.1",
    "@types/vscode": "^1.47.0",
