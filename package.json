  "scripts": {
    "build": "esno scripts/build.ts",
    "dev": "pnpm build -- --watch",
    "lint": "eslint .",
    "prepare": "esno scripts/prepare.ts",
    "release": "bumpp --tag --push --commit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@iconify/iconify": "^2.2.1",
    "@iconify/json": "^2.1.99",
