          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "JSON paths for custom collection"
        },
        "iconify.customCollectionIdsMap": {
          "type": "object",
          "items": {
            "type": "string"
          },
          "default": {},
          "description": "Collection IDs Map for collection name alias, e.g. { 'mc': 'mingcute' }"
        },
        "iconify.customAliasesJsonPaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "JSON paths for custom aliases"
        },
        "iconify.customAliasesOnly": {
          "type": "boolean",
          "default": false,
          "description": "Only use the icon aliases. Non aliased icons will be ignored."
        },
        "iconify.preview.include": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/*.*"
          ],
          "description": "Glob patterns for file paths to enable icon preview"
        },
        "iconify.preview.exclude": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Glob patterns for file paths to disable icon preview"
        }
