{
  "name": "iconify",
  "displayName": "Iconify IntelliSense",
  "description": "Intelligent Iconify previewing and searching for VS Code",
  "publisher": "antfu",
  "version": "0.0.5",
  "license": "MIT",
  "private": true,
  "preview": true,
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "activationEvents": [
    "workspaceContains:package.json",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:handlebars",
    "onLanguage:svelte",
    "onLanguage:html",
    "onLanguage:css"
  ],
  "keywords": [
    "icons",
    "iconify"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-iconify"
  },
  "homepage": "https://github.com/antfu/vscode-iconify",
  "bugs": "https://github.com/antfu/vscode-iconify/issues",
  "icon": "res/logo.png",
  "main": "dist/index.js",
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "snippets/html.json"
      },
      {
        "language": "vue",
        "path": "snippets/html.json"
      }
    ],
    "commands": [
      {
        "command": "iconify.toggle-annotations",
        "category": "Iconify",
        "title": "Toggle Annotations"
      },
      {
        "command": "iconify.toggle-inplace",
        "category": "Iconify",
        "title": "Toggle In-place Mode"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Iconify IntelliSense",
      "properties": {
        "iconify.inplace": {
          "type": "boolean",
          "default": true,
          "description": "Use icon to replace the icon name."
        },
        "iconify.annotations": {
          "type": "boolean",
          "default": true,
          "description": "Enabled Iconify inline annotations"
        },
        "iconify.color": {
          "type": "string",
          "default": "auto",
          "description": "Icon color hex for inline displaying."
        },
        "iconify.delimiter": {
          "type": "string",
          "default": ":",
          "description": "Delimiter for separating between collection id and icon id"
        },
        "iconify.includes": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "mdi",
              "mdi-light",
              "ic",
              "uil",
              "uim",
              "bx",
              "ri",
              "la",
              "jam",
              "ion",
              "bi",
              "clarity",
              "carbon",
              "cil",
              "gg",
              "tabler",
              "fa-solid",
              "fa-regular",
              "vaadin",
              "icomoon-free",
              "eva",
              "dashicons",
              "entypo",
              "zondicons",
              "flat-color-icons",
              "octicon",
              "codicon",
              "ant-design",
              "feather",
              "fe",
              "system-uicons",
              "gridicons",
              "heroicons-outline",
              "heroicons-solid",
              "ps",
              "el",
              "foundation",
              "typcn",
              "subway",
              "raphael",
              "icons8",
              "wpf",
              "iwwa",
              "topcoat",
              "ei",
              "bytesize",
              "maki",
              "oi",
              "et",
              "fa",
              "zmdi",
              "whh",
              "si-glyph",
              "ls",
              "simple-line-icons",
              "flat-ui",
              "vs",
              "websymbol",
              "il",
              "bpmn",
              "fontelico",
              "noto",
              "noto-v1",
              "twemoji",
              "openmoji",
              "emojione",
              "emojione-monotone",
              "emojione-v1",
              "fxemoji",
              "logos",
              "cib",
              "simple-icons",
              "fa-brands",
              "brandico",
              "entypo-social",
              "cryptocurrency",
              "map",
              "geo",
              "cif",
              "wi",
              "medical-icon"
            ]
          },
          "default": null,
          "description": "Collection IDs to be included for detection"
        },
        "iconify.excludes": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "mdi",
              "mdi-light",
              "ic",
              "uil",
              "uim",
              "bx",
              "ri",
              "la",
